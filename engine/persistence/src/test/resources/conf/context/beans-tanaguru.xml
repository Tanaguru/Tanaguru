<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="auditDAO" class="org.opens.tanaguru.entity.dao.audit.AuditDAOImpl"/>
    <bean id="contentDAO" class="org.opens.tanaguru.entity.dao.audit.ContentDAOImpl"/>
    <bean id="evidenceDAO" class="org.opens.tanaguru.entity.dao.audit.EvidenceDAOImpl"/>
    <bean id="evidenceElementDAO" class="org.opens.tanaguru.entity.dao.audit.EvidenceElementDAOImpl"/>
    <bean id="processRemarkDAO" class="org.opens.tanaguru.entity.dao.audit.ProcessRemarkDAOImpl"/>
    <bean id="processResultDAO" class="org.opens.tanaguru.entity.dao.audit.ProcessResultDAOImpl"/>
    <bean id="criterionDAO" class="org.opens.tanaguru.entity.dao.reference.CriterionDAOImpl"/>
    <bean id="levelDAO" class="org.opens.tanaguru.entity.dao.reference.LevelDAOImpl"/>
    <bean id="referenceDAO" class="org.opens.tanaguru.entity.dao.reference.ReferenceDAOImpl"/>
    <bean id="testDAO" class="org.opens.tanaguru.entity.dao.reference.TestDAOImpl">
        <property name="goldLevelCodeList">
            <list>
                <value>Or</value>
            </list>
        </property>
        <property name="silverLevelCodeList">
            <list>
                <value>Ar</value>
            </list>
        </property>
    </bean>
    <bean id="themeDAO" class="org.opens.tanaguru.entity.dao.reference.ThemeDAOImpl"/>
    <bean id="scopeDAO" class="org.opens.tanaguru.entity.dao.reference.ScopeDAOImpl"/>
    <bean id="webresourceDAO" class="org.opens.tanaguru.entity.dao.subject.WebResourceDAOImpl"/>
    <bean id="pageDAO" class="org.opens.tanaguru.entity.dao.subject.PageDAOImpl"/>
    <bean id="siteDAO" class="org.opens.tanaguru.entity.dao.subject.SiteDAOImpl"/>
    <bean id="parameterDAO" class="org.opens.tanaguru.entity.dao.parameterization.ParameterDAOImpl"/>
    <bean id="parameterElementDAO" class="org.opens.tanaguru.entity.dao.parameterization.ParameterElementDAOImpl"/>
    <bean id="parameterFamilyDAO" class="org.opens.tanaguru.entity.dao.parameterization.ParameterFamilyDAOImpl"/>

</beans>