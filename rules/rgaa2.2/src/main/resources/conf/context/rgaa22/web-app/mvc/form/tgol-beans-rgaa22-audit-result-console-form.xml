        <property name="errorI18nKey" value="result.theme-error"/>
        <property name="value" value="RGAA30"/>
        <property name="selectElementBuilderMap">
            <map>
                <entry key="theme">
                    <list>
                        <ref bean="rgaa22-AllThemeSelectElementBuilder"/>
                        <ref bean="rgaa22-1ThemeSelectElementBuilder"/>
                        <ref bean="rgaa22-2ThemeSelectElementBuilder"/>
                        <ref bean="rgaa22-3ThemeSelectElementBuilder"/>
