        <property name="i18nKey" value="result.theme"/>
        <property name="errorI18nKey" value="result.theme-error"/>
        <property name="value" value="Rgaa30"/>
        <property name="selectElementBuilderMap">
            <map>
                <entry key="theme">
                    <list>
                        <ref bean="rgaa30AllThemeSelectElementBuilder"/>
                            <ref bean="rgaa30-1ThemeSelectElementBuilder"/>
                            <ref bean="rgaa30-2ThemeSelectElementBuilder"/>
                            <ref bean="rgaa30-3ThemeSelectElementBuilder"/>
                            <ref bean="rgaa30-4ThemeSelectElementBuilder"/>
